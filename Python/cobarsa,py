from Crypto.Util.number import long_to_bytes

# Nilai n, e, dan chipertext yang diberikan
n = 
e = 65537
c = 


def decrypt(n, e, chipertext):
    # Mendapatkan nilai d (eksponen privat)
    d = pow(e, -1, n - 1)
    # Mendekripsi chipertext menjadi plainteks
    plaintext = pow(chipertext, d, n)
    # Mengkonversi plainteks ke bentuk bytes
    plaintext_bytes = long_to_bytes(plaintext)
    return plaintext_bytes


# Dekripsi chipertext
decrypted_message = decrypt(n, e, chipertext)
print("Decrypted message:", decrypted_message.decode)
